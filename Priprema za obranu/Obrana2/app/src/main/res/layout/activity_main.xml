<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/fineButton"
            android:layout_width="121dp"
            android:layout_height="65dp"
            android:layout_marginStart="62dp"
            android:layout_marginTop="48dp"
            android:text="Fine"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/internetButton" />

        <Button
            android:id="@+id/coarseButton"
            android:layout_width="121dp"
            android:layout_height="65dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="45dp"
            android:text="Coarse"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/goToLocationButton"
            android:layout_width="121dp"
            android:layout_height="65dp"
            android:layout_marginStart="144dp"
            android:layout_marginBottom="372dp"
            android:text="Go To Location"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/internetButton"
            android:layout_width="121dp"
            android:layout_height="65dp"
            android:layout_marginStart="62dp"
            android:layout_marginTop="80dp"
            android:text="Internet"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/contactsButton"
            android:layout_width="121dp"
            android:layout_height="65dp"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="45dp"
            android:text="Contacts"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/coarseButton" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>